  },
  "engines": {
    "node": "23.3.0"
  },
  "dependencies": {
    "@elizaos/adapter-sqlite": "^0.1.7",
    "@elizaos/core": "^0.1.7",
    "@tavily/core": "^0.0.2",
    "agent-twitter-client": "^0.0.18",
    "better-sqlite3": "^11.7.2",
    "cron-parser": "^4.9.0",
